{"ast":null,"code":"import { api } from './Helpers';\nexport default {\n  name: 'Member-Page',\n  data() {\n    return {\n      email: '',\n      password: '',\n      rememberPassword: false\n    };\n  },\n  methods: {\n    signIn() {\n      if (!this.rememberPassword) {\n        // กรุณาเลือก checkbox ก่อน Sign In\n        this.$flash('Please check the Remember password checkbox before signing in', 'error');\n        return;\n      }\n      const payload = {\n        email: this.email,\n        password: this.password\n      };\n      api.signIn(payload).then(response => {\n        // กระบวนการหลังจากเข้าสู่ระบบสำเร็จ\n        console.log(response.data);\n      }).catch(error => {\n        // กระบวนการหลังจากเข้าสู่ระบบไม่สำเร็จ\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["api","name","data","email","password","rememberPassword","methods","signIn","$flash","payload","then","response","console","log","catch","error"],"sources":["src/views/Member.vue"],"sourcesContent":["<template>\n    <div class=\"container main \" id=\"container-sign-in\">\n      <div class=\"text-dark fw-bold fs-1 text-center mt-4 mb-1\">\n        Sign In to Camera Box\n      </div>\n      <div class=\"card\" id=\"card-sign-in\">\n        <div class=\"card-body\">\n          <form @submit.prevent=\"signIn\">\n            <div class=\"mb-3\">\n              <div class=\"form-group was-validated\">\n                <label for=\"Email\" class=\"form-label\">Email</label>\n                <input\n                  type=\"text\"\n                  v-model=\"email\"\n                  class=\"form-control\"\n                  placeholder=\"Enter your username ...\"\n                  required\n                />\n              </div>\n              <div class=\"invalid-feedback\">\n                Please enter your email.\n              </div>\n            </div>\n            <div class=\"mb-3\">\n              <div class=\"form-group was-validated\">\n                <label for=\"pass\" class=\"form-label\">Password</label>\n                <input\n                  type=\"password\"\n                  v-model=\"password\"\n                  class=\"form-control\"\n                  placeholder=\"Enter your password ...\"\n                  required\n                />\n              </div>\n              <div class=\"invalid-feedback\">\n                Please enter your password.\n              </div>\n            </div>\n            <div class=\"mb-3\">\n              <div class=\"form-group form-check\">\n                <input\n                  type=\"checkbox\"\n                  v-model=\"rememberPassword\"\n                  id=\"agree-check\"\n                  class=\"form-check-input me-2\"\n                />\n                <label for=\"agree-check\" class=\"form-check-label\" id=\"check-label\">\n                  Remember password\n                </label>\n              </div>\n            </div>\n            <button type=\"submit\" class=\"btn btn-light rounded-3 border\" id=\"btn-sign-in\">\n              SIGN IN\n            </button>\n          </form>\n        </div>\n      </div>\n      <p class=\"text-center border border-dark rounded-3 p-2\" id=\"create-box\">\n        New to Camera Box?\n        <router-link to=\"/signUp\" class=\"text-success\"> Create account</router-link>\n      </p>\n    </div>\n  </template>\n  \n  <script>\n  import { api } from './Helpers';\n  \n  export default {\n    name: 'Member-Page',\n    data() {\n      return {\n        email: '',\n        password: '',\n        rememberPassword: false,\n      };\n    },\n    methods: {\n      signIn() {\n        if (!this.rememberPassword) {\n          // กรุณาเลือก checkbox ก่อน Sign In\n          this.$flash('Please check the Remember password checkbox before signing in', 'error');\n          return;\n        }\n  \n        const payload = {\n          email: this.email,\n          password: this.password,\n        };\n  \n        api.signIn(payload)\n          .then((response) => {\n            // กระบวนการหลังจากเข้าสู่ระบบสำเร็จ\n            console.log(response.data);\n          })\n          .catch((error) => {\n            // กระบวนการหลังจากเข้าสู่ระบบไม่สำเร็จ\n            console.error(error);\n          });\n      },\n    },\n  };\n  </script>\n<style scoped>\n    #card-sign-in, #create-box{\n    max-width: 600px;\n    margin: 10px;\n}\n\n#label-sign-in{\n    font-size: 15px;\n    font-weight: 250;\n    margin: 0 auto;\n    margin-bottom: 10px;\n}\n#container-sign-in{\n    margin: 0 auto;\n    margin-top: 10px;\n    margin-bottom: 10px;\n    padding: 0;\n    width: 100%;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n}\n\n#username, #pass {\n    max-width:500px;\n    height: 2.5rem;\n    border: 1px solid #ccc;\n    border-radius: 5px;\n    padding: 0.5rem;\n    margin-bottom: 10px;\n}\n</style>"],"mappings":"AAiEA,SAAAA,GAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,QAAA;MACAC,gBAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACA,UAAAF,gBAAA;QACA;QACA,KAAAG,MAAA;QACA;MACA;MAEA,MAAAC,OAAA;QACAN,KAAA,OAAAA,KAAA;QACAC,QAAA,OAAAA;MACA;MAEAJ,GAAA,CAAAO,MAAA,CAAAE,OAAA,EACAC,IAAA,CAAAC,QAAA;QACA;QACAC,OAAA,CAAAC,GAAA,CAAAF,QAAA,CAAAT,IAAA;MACA,GACAY,KAAA,CAAAC,KAAA;QACA;QACAH,OAAA,CAAAG,KAAA,CAAAA,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}