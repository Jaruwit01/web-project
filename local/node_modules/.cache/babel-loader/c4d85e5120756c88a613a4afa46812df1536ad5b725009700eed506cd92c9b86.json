{"ast":null,"code":"import api from \"../helpers/Helpers.js\";\nimport Cards from \"../components/Cards.vue\";\nexport default {\n  name: \"admin-page\",\n  data() {\n    return {\n      items: []\n    };\n  },\n  components: {\n    Cards\n  },\n  methods: {\n    async onDestroy(id) {\n      const sure = window.confirm(\"Are you sure?\");\n      if (!sure) return;\n      await api.deleteItem(id);\n      this.flash('Item deleted successfully!', 'success');\n      const newItems = this.items.filter(item => item._id !== id);\n      this.items = newItems;\n    },\n    async mounted() {\n      this.items = await api.getItems();\n    }\n  }\n};","map":{"version":3,"names":["api","Cards","name","data","items","components","methods","onDestroy","id","sure","window","confirm","deleteItem","flash","newItems","filter","item","_id","mounted","getItems"],"sources":["src/views/Camera.vue"],"sourcesContent":["<template>\n  <div>\n    <h1 class=\"text-center mt-4\">Camera Item</h1>\n    <div v-if=\"item.length > 0\">\n      <div v-for=\"(item, i) in items\" :key=\"i\">\n        <Cards>\n          <template v-slot:pictur>\n            <img />\n          </template>\n          <template v-slot:names>\n            <p class=\"card-text fs-5\">{{ item.name }}</p>\n          </template>\n\n          <template v-slot:type>\n            <p class=\"card-title\">Type: {{ item.type }}</p>\n          </template>\n\n          <template v-slot:content>\n            <ul class=\"fw-light fs-6\">\n              <li v-for=\"(Item, j) in item.details.split('')\" :key=\"j\">\n                {{ Item }}\n              </li>\n            </ul>\n          </template>\n          <button class=\"btn btn-light rounded-3 bg-danger text-white\" @click.prevent=\"onDestroy(item._id)\"><a :href=\"`/admin-page/${item._id}`\">delete</a></button>\n        </Cards>\n      </div>\n    </div>\n    <div v-else>\n        <h1 class=\"text-center mt-4\">No Item</h1>\n    </div>\n  </div>\n</template>\n<script>\nimport api from \"../helpers/Helpers.js\";\nimport Cards from \"../components/Cards.vue\";\nexport default {\n  name: \"admin-page\",\n    data() {\n        return {\n        items: [],\n        };\n    },\n  components: {\n    Cards,\n  },\n    methods: {\n        async onDestroy(id) {\n            const sure = window.confirm(\"Are you sure?\");\n            if(!sure) return;\n            await api.deleteItem(id);\n            this.flash('Item deleted successfully!', 'success');\n            const newItems = this.items.filter(item => item._id !== id);\n            this.items = newItems;\n        },\n        async mounted() {\n            this.items = await api.getItems();\n        },\n    },\n};\n</script>\n<style scoped></style>\n"],"mappings":"AAkCA,OAAAA,GAAA;AACA,OAAAC,KAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;IACA;EACA;EACAC,UAAA;IACAJ;EACA;EACAK,OAAA;IACA,MAAAC,UAAAC,EAAA;MACA,MAAAC,IAAA,GAAAC,MAAA,CAAAC,OAAA;MACA,KAAAF,IAAA;MACA,MAAAT,GAAA,CAAAY,UAAA,CAAAJ,EAAA;MACA,KAAAK,KAAA;MACA,MAAAC,QAAA,QAAAV,KAAA,CAAAW,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,GAAA,KAAAT,EAAA;MACA,KAAAJ,KAAA,GAAAU,QAAA;IACA;IACA,MAAAI,QAAA;MACA,KAAAd,KAAA,SAAAJ,GAAA,CAAAmB,QAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}